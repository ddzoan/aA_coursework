<li style="list-style-type: none;">
  <div class="col-md-4">
    <%= note.note %>
  </div>
  <div class="col-md-4">
    <%= note.user.email %>
  </div>
  <div class="col-md-4">
    <% if note.user == current_user %>
      <%= button_to 'Delete', note_url(note), method: :delete, class: 'btn btn-xs btn-danger' %>
    <% else %>
      -
    <% end %>
  </div>
</li>
